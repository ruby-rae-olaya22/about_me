	"item": [
		{
			"name": "Create a playlist",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"// ============================================",
							"// Create Playlist - Post-Response Tests",
							"// ============================================",
							"",
							"const jsonData = pm.response.json();",
							"const requestBody = JSON.parse(pm.request.body.raw);",
							"",
							"// 1. Status Code Validation",
							"pm.test(\"Status code is 201 Created\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"// 2. Required Fields Validation",
							"pm.test(\"Response has all required fields\", function () {",
							"    pm.expect(jsonData).to.have.property(\"id\");",
							"    pm.expect(jsonData).to.have.property(\"created_at\");",
							"    pm.expect(jsonData).to.have.property(\"creator\");",
							"    pm.expect(jsonData).to.have.property(\"name\");",
							"    pm.expect(jsonData).to.have.property(\"description\");",
							"    pm.expect(jsonData).to.have.property(\"image\");",
							"    pm.expect(jsonData).to.have.property(\"songs\");",
							"    pm.expect(jsonData).to.have.property(\"tags\");",
							"    pm.expect(jsonData).to.have.property(\"share\");",
							"});",
							"",
							"// 3. Response Data Matches Request Data",
							"pm.test(\"Name matches request data\", function () {",
							"    pm.expect(jsonData.name).to.eql(requestBody.name);",
							"});",
							"",
							"pm.test(\"Description matches request data\", function () {",
							"    pm.expect(jsonData.description).to.eql(requestBody.description);",
							"});",
							"",
							"pm.test(\"Tags count matches request data\", function () {",
							"    // Parse tags if returned as string",
							"    const responseTags = typeof jsonData.tags === \"string\" ? JSON.parse(jsonData.tags) : jsonData.tags;",
							"    pm.expect(responseTags).to.be.an(\"array\");",
							"    pm.expect(responseTags.length).to.eql(requestBody.tags.length);",
							"});",
							"",
							"// 4. Data Type Validations",
							"pm.test(\"ID is a valid string\", function () {",
							"    pm.expect(jsonData.id).to.be.a(\"string\");",
							"    pm.expect(jsonData.id).to.not.be.empty;",
							"});",
							"",
							"pm.test(\"Created_at is a string\", function () {",
							"    pm.expect(jsonData.created_at).to.be.a(\"string\");",
							"    pm.expect(jsonData.created_at).to.not.be.empty;",
							"});",
							"",
							"pm.test(\"Songs is an array or parseable array string\", function () {",
							"    if (typeof jsonData.songs === \"string\") {",
							"        const parsedSongs = JSON.parse(jsonData.songs);",
							"        pm.expect(parsedSongs).to.be.an(\"array\");",
							"        pm.expect(parsedSongs.length).to.eql(9);",
							"    } else {",
							"        pm.expect(jsonData.songs).to.be.an(\"array\");",
							"        pm.expect(jsonData.songs.length).to.eql(9);",
							"    }",
							"});",
							"",
							"pm.test(\"Creator is a string\", function () {",
							"    pm.expect(jsonData.creator).to.be.a(\"string\");",
							"});",
							"",
							"// 5. Share URL Validation",
							"pm.test(\"Share URL is properly formatted\", function () {",
							"    pm.expect(jsonData.share).to.be.a(\"string\");",
							"    pm.expect(jsonData.share).to.match(/^https?:\\/\\/.+/);",
							"});",
							"",
							"// 6. Response Time Validation",
							"pm.test(\"Response time is under 2000ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);",
							"});",
							"",
							"pm.test('Content-Type header is application/json', function () {",
							"    pm.response.to.have.header('Content-Type');",
							"    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');",
							"})",
							"",
							"pm.test('Playlist ID is saved to collection variable', function () {",
							"    const jsonData = pm.response.json();",
							"    if (jsonData.id) {",
							"        pm.collectionVariables.set('playlistId', jsonData.id);",
							"        pm.expect(pm.collectionVariables.get('playlistId')).to.eql(jsonData.id);",
							"    }",
							"})"
						]
					}
				}
			],
